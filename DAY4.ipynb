   "source": [
    "# If even, print both middle elements  and if odd print print the middle element"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "\n"
     ]
    }
   ],
   "source": [
    "a=input()\n",
    "z=len(str(a))\n",
    "\n",
    "middle=round(z/2)\n",
    "p=middle-1\n",
    "if( z%2==0):\n",
    "    print(a[p:middle+1])\n",
    "elif (z%2!=0):\n",
    "    print(a[middle])\n"
   ]
  },
